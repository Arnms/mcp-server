{
  "name": "mcp-server",
  "version": "0.0.1",
  "description": "mcp-server MCP server",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "mcp-server": "./dist/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node --esm src/index.ts",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "start:advanced": "MCP_SERVER_TYPE=advanced node dist/index.js",
    "dev:advanced": "MCP_SERVER_TYPE=advanced ts-node --esm src/index.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest"
  },
  "author": "Arnms",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.1",
    "axios": "^1.9.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "prettier": "^3.5.3",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.19.0"
  }
}
